@model IEnumerable<OrderSystem.Models.VendorItem>

@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>

        </div>
    </section>
}

<form action="" method="post" enctype="multipart/form-data">
    <label for="file">Filename:</label>
    <input type="file" name="file" id="file" />
    <input type="submit" />
</form>

<ul>
    @foreach (var vendorItem in Model) {
        <li>@vendorItem.ItemName, @vendorItem.ItemPrice</li>    
    }
</ul>
